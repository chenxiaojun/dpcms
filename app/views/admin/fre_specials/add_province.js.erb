var html = "<%= j(render('province_form', remote: true)) %>";
// 为了dialog有标题
$(html).dialog({
    modal: true,
    width: '50%',
    title: '新增特定地区'
});

$(".fre_province_select span").click(
    function(){
        selected = $(this).data('selected');
        $(this).data('selected', !selected);
        if(!selected) {
            $(this).addClass('blue');
        }else{
            $(this).removeClass('blue')
        }
    }
);